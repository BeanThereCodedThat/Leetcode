class Solution 
{
    public int singleNumber(int[] nums) 
    {
        Arrays.sort(nums);
        int i=0,a=-1;
        if(nums.length==1)
        return nums[0];
        a=nums[nums.length-1];
        for(i=0;i<nums.length-2;i=i+3)
        {
            if(nums[i] != nums[i+2])
                {
                return nums[i];
                }
             
        }
        return a;
    }
}
