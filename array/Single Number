class Solution 
{
    public int singleNumber(int[] nums) 
    {
        Arrays.sort(nums);
        int i=0,j=1,a=-1;
        a=nums[nums.length-1];
        while(j<nums.length)
        {
            if((nums[i] ^ nums[j])==0)
                {
                i=i+2;
                j=j+2;
                }
            else
            {
            a=nums[i];
            break;
            }        
        }
        return a;
    }
}
