import java.util.*;
class Solution 
{
    public int findMaxConsecutiveOnes(int[] nums) 
    {
        int i=0,j=0,c=0;
        while(j<nums.length)
        {
            if(nums[j]==0)
            {
                i=j+1;
                j++;
            }
            else
            {
                c=Math.max(c,j-i+1);
                j=j+1;
            }

        }
        return c;
    }
}
